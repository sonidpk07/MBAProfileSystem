<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping  package="com.spring.mvc.pojo">
<class name="AddCourseBean" table="course"> 
<id name="id" column="id"></id>
<property name="prgm" column="program"></property>
<property name="prefix" column="prefix"></property>
<property name="name" column="name"></property>
<property name="concentration" column="concentration"></property>
<property name="status" column="status"></property>
<property name="type" column="type"></property>
</class>
</hibernate-mapping>
